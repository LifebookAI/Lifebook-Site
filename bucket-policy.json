{
  "Statement": [
    {
      "Action": [
        "s3:GetObject",
        "s3:GetObjectVersion"
      ],
      "Effect": "Allow",
      "Condition": {
        "StringEquals": {
          "AWS:SourceArn": "arn:aws:cloudfront::/E2D7FJLA6YQUNP"
        }
      },
      "Sid": "AllowCloudFrontReadViaOAC",
      "Principal": {
        "Service": "cloudfront.amazonaws.com"
      },
      "Resource": "arn:aws:s3:::lifebook.ai/*"
    },
    {
      "Action": "s3:*",
      "Effect": "Deny",
      "Condition": {
        "Bool": {
          "aws:SecureTransport": "false"
        }
      },
      "Sid": "DenyInsecureTransport",
      "Principal": "*",
      "Resource": [
        "arn:aws:s3:::lifebook.ai",
        "arn:aws:s3:::lifebook.ai/*"
      ]
    },
    {
      "Action": "s3:PutObject",
      "Effect": "Deny",
      "Condition": {
        "StringNotEquals": {
          "s3:x-amz-server-side-encryption": "aws:kms"
        }
      },
      "Sid": "DenyNonKmsPuts",
      "Principal": "*",
      "Resource": "arn:aws:s3:::lifebook.ai/*"
    },
    {
      "Action": "s3:PutObject",
      "Effect": "Deny",
      "Condition": {
        "StringNotEquals": {
          "s3:x-amz-server-side-encryption-aws-kms-key-id": "arn:aws:kms:us-east-1:354630286254:key/583a1a4c-efbc-486d-8025-66577c04116a"
        }
      },
      "Sid": "DenyWrongKmsKey",
      "Principal": "*",
      "Resource": "arn:aws:s3:::lifebook.ai/*"
    }
  ],
  "Version": "2012-10-17"
}
